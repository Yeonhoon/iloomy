<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias alias="delivery" type="com.mycompany.webapp.dto.DeliveryDTO"/>
		<typeAlias alias="member" type="com.mycompany.webapp.dto.UserDTO"/>
		<typeAlias alias="AddressDTO" type="com.mycompany.webapp.dto.AddressDTO"/>
		<typeAlias alias="notice" type="com.mycompany.webapp.dto.NoticeDTO"/>
		<typeAlias alias="pager" type="com.mycompany.webapp.dto.PagerDTO"/>
		<typeAlias alias="item" type="com.mycompany.webapp.dto.ItemsDTO"/>
		<typeAlias alias="order" type="com.mycompany.webapp.dto.OrdersDTO"/>
		<typeAlias alias="orderitem" type="com.mycompany.webapp.dto.OrderItemsDTO"/>
	</typeAliases>
	
	<typeHandlers>        
	     <typeHandler handler="org.apache.ibatis.type.EnumOrdinalTypeHandler" javaType="com.mycompany.webapp.dto.OrderStatus" />      
	     <typeHandler handler="org.apache.ibatis.type.EnumOrdinalTypeHandler" javaType="com.mycompany.webapp.dto.DeliveryStatus" />    
	 </typeHandlers>
	
</configuration>